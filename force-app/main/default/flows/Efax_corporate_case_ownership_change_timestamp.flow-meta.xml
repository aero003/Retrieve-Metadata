        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(

         ISBLANK([Case].Ownership_Date_Time__c ),
         NOT(
                  ISBLANK([Case].RecordTypeId)
          ),
         ISCHANGED([Case].OwnerId),
         ([Case].OwnerId &lt;&gt;  &quot;00560000001EuskAAC&quot;),
         [Case].Owned_by_a_User__c = true
         
         
) </stringValue>
            </value>
        </processMetadataValues>
