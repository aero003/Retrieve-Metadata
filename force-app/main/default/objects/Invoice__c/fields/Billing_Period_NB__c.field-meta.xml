<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Billing_Period_NB__c</fullName>
    <description>Translates &quot;One-off&quot; to &quot;Monthly&quot; for Oracle purposes.</description>
    <externalId>false</externalId>
    <formula>CASE(Billing_Period__c, 
&quot;Annual&quot;,&quot;Annual&quot;, 
&quot;Quarterly&quot;,&quot;Quarterly&quot;, 
&quot;Bi-Annual&quot;,&quot;Bi-Annual&quot;, 
&quot;Monthly&quot;,&quot;Monthly&quot;, 
&quot;One-off&quot;,&quot;Monthly&quot;, 
null 
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Billing Period NB</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
