    <script>
        var statusMap = new Map();

        function downloadLOA(envalopid) {

            Visualforce.remoting.Manager.invokeAction(
                '{!$RemoteAction.J2_ISP_Tool_Controller.downloadLOA}',
                envalopid,
                function(result, event) {
                    if (event.statusCode == 200) {
                        window.open(event.result, '_blank');
                        //return event.result;
                    }
                }, {
                    escape: false
                })
        }

        function getEnvelopStatus(envalopid) {
            Visualforce.remoting.Manager.invokeAction(
                '{!$RemoteAction.J2_ISP_Tool_Controller.getEnvelopStatus}',
                envalopid,
                function(result, event) {
                    if (event.statusCode == 200) {
                        //var map1=new Map();
                        Object.keys(event.result).forEach(function(key) {
                            //value = event.result[key];
                            $('#td' + key).html(event.result[key]);
                            statusMap.put(key, event.result[key]);

                        });

                        return statusMap;

                    }
                }, {
                    escape: false
                })
        }

        $(function() {
            //save url of api calls in data
            $("body").data("iframe_url", "{!URLFOR('apex/J2_ISP_SFDC_LOGIN')}");
            $("body").data("apiurl", "{!$Setup.J2_ISP_ToolCustomSetting__c.J2_ISP_ApiEndpoint__c}");
            $("body").data("faxforceEnvironment", "{!$Setup.J2_ISP_ToolCustomSetting__c.J2_FaxForce_Environment__c}");
            $("body").data("sfAccountid", "{!$CurrentPage.parameters.id}");
            $("body").data("apiurl", "{!$Setup.J2_ISP_ToolCustomSetting__c.J2_ISP_ApiEndpoint__c}");
            var ispusertoken = "{!$User.ISP_CS_TOOL_TOKEN__c}";
            if (ispusertoken !== null) {
                $.cookie("ispusertoken", ispusertoken, {
                    path: '/',
                });
                $("body").data("ispuserToken", ispusertoken);
            } else {
                $.removeCookie("ispuserToken");
                $("body").data("ispuserToken", "");
            }
            var ispldapusername = "{!$User.ISP_CS_TOOL_LDAP_USERNAME__c}";
            if (ispldapusername !== null) {
                $.cookie("ispldapusername", ispldapusername, {
                    path: '/',
                });
                $("body").data("ispldapusername", ispldapusername);
            } else {
                $.removeCookie("ispldapusername");
                $("body").data("ispldapusername", "");
            }
            $(".bPageFooter").prepend(createVersionFooter);

        });
        var getContactId = function() {
            sforce.connection.sessionId = "{!$Api.Session_ID}";
            var acid = $("body").data("sfAccountid");
            var qr = sforce.connection.query("select id from Contact where accountid ='" + acid + "'");
            var contactid = qr.getArray("records");

            if (contactid.length == 1) {
                $("body").data("sfContactid", contactid[0].Id);
            } else {
                $("body").data("sfContactid", "");
            }

        }
        var createVersionFooter = function() {
            var footerString = "<div>";
            footerString += "<span class='brandQuaternaryFgr'>UI version :  {!$Setup.J2_ISP_ToolCustomSetting__c.J2_ISP_UIVersion__c} </span> | ";
            footerString += "<a href='{!$Setup.J2_ISP_ToolCustomSetting__c.J2_ISP_ApiEndpoint__c}' class='brandQuaternaryFgr' target='_blank' title='FFAPI Version (New Window)'>FFAPI Version : {!$Setup.J2_ISP_ToolCustomSetting__c.J2_ISP_ApiVersion__c}</a> | ";
            footerString += "<span>CSAPI Version : {!$Setup.J2_ISP_ToolCustomSetting__c.J2_ISP_CS_ApiVersion__c}</span> | ";
            footerString += "<a href='{!$Setup.J2_ISP_ToolCustomSetting__c.J2_ISP_ATFSendpoint__c}' class='brandQuaternaryFgr' target='_blank' title='ATS API Version (New Window)'> ATS API Version : {!$Setup.J2_ISP_ToolCustomSetting__c.J2_ISP_ATFSApiVersion__c}</a>";
            footerString += "</div>";
            return footerString;
        }
    </script>
