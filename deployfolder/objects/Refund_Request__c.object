    <validationRules>
        <fullName>Credit_Reason_must_be_populated</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
 RecordTypeID__c =&quot;012600000009bWj&quot;,
 Requested_Credit_Amount__c &gt;0,
 ISPICKVAL( Reason_for_Credit__c,&quot;&quot; )
)</errorConditionFormula>
        <errorDisplayField>Reason_for_Credit__c</errorDisplayField>
        <errorMessage>If credit is requested, then Reason for Credit cannot be blank.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RefundAmount_CreditAmount_mandatory</fullName>
        <active>true</active>
        <description>Only one of refund amount, credit amount mandatory.</description>
        <errorConditionFormula>OR(
	AND(
		OR(ISBLANK( Requested_Credit_Amount__c ),(Requested_Credit_Amount__c =0)) ,
		OR(ISBLANK( Requested_Refund_Amount__c ),(Requested_Refund_Amount__c =0))
	)
	,
	AND(
		AND(NOT(ISBLANK( Requested_Credit_Amount__c )),Requested_Credit_Amount__c !=0 ) ,
		AND(NOT(ISBLANK( Requested_Refund_Amount__c )),Requested_Refund_Amount__c !=0 )
	)
)</errorConditionFormula>
        <errorMessage>Please enter Requested Credit Amount or Requested Refund Amount.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Refund_Amount_must_be_populated_Intl</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
 RecordTypeId = &quot;012600000009cYz&quot;,
 
 Requested_Refund_Amount__c =0
)</errorConditionFormula>
        <errorDisplayField>Requested_Refund_Amount__c</errorDisplayField>
        <errorMessage>Requested Refund Amount is required to have an amount.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Refund_Amount_must_be_populated_Micro</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
 RecordTypeId = &quot;012600000009beN&quot;,
 
 Requested_Refund_Amount__c =0
)</errorConditionFormula>
        <errorDisplayField>Requested_Refund_Amount__c</errorDisplayField>
        <errorMessage>Requested Refund Amount is required to have an amount.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Refund_or_Credit_Amount_must_be_populate</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
 RecordTypeId = &quot;012600000009bWj&quot;,
 Requested_Credit_Amount__c =0,
 Requested_Refund_Amount__c =0
)</errorConditionFormula>
        <errorDisplayField>Requested_Credit_Amount__c</errorDisplayField>
        <errorMessage>Either Requested Credit Amount or Requested Refund Amount are required to have an amount.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Refund_Reason_must_be_populated</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
 RecordTypeID__c =&quot;012600000009bWj&quot;,
 Requested_Refund_Amount__c &gt;0,
 ISPICKVAL( Reason_for_Refund__c,&quot;&quot; )
)</errorConditionFormula>
        <errorDisplayField>Reason_for_Refund__c</errorDisplayField>
        <errorMessage>If refund is requested, then Reason for Refund cannot be blank.</errorMessage>
    </validationRules>
