    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>&quot;WG Call - New Account:&quot;&amp;[Account].Account_ID_18__c </stringValue>
            </value>
        </processMetadataValues>
        <name>formula_2_myRule_1_A1_0388839238</name>
        <dataType>String</dataType>
        <expression>&quot;WG Call - New Account:&quot;&amp;{!myVariable_current.Account_ID_18__c}</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(
		CONTAINS(UPPER([Account].Company_Key__c), &quot;VCO&quot;),  
		NOT(CONTAINS(UPPER([Account].Name) ,&quot;QATEST&quot;)),   
		OR(
			AND( 
				NOT(CONTAINS(UPPER([Account].Rate_Plan_Description__c) ,&quot;PRO_12.99_2X&quot;)),
				NOT(CONTAINS(UPPER([Account].Rate_Plan_Description__c) ,&quot;PRO_14.99_2X&quot;)),
				NOT(CONTAINS(UPPER([Account].Rate_Plan_Description__c) ,&quot;SMB1_29.99_5X&quot;)),				
				NOT(CONTAINS(UPPER([Account].Rate_Plan_Description__c) ,&quot;ITUNES&quot;)), 
				NOT(CONTAINS(UPPER([Account].Rate_Plan_Description__c) ,&quot;ANDROID&quot;))  
			), ISBLANK([Account].Rate_Plan_Description__c) )    , 
		NOT(CONTAINS(UPPER([Account].Email_Addresses_1__c) ,&quot;@J2.COM&quot;)), 
		NOT(ISPICKVAL([Account].Status__c, &quot;Inactive&quot;)), 
		NOT(ISPICKVAL([Account].Status__c, &quot;Deactivated&quot;)), 
		NOT(ISPICKVAL([Account].Status__c, &quot;Suspended&quot;)), 
		NOT(ISPICKVAL([Account].Status__c, &quot;Closed&quot;)) 
	) </stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_1</name>
        <dataType>Boolean</dataType>
        <expression>AND(
		CONTAINS(UPPER({!myVariable_current.Company_Key__c}), &quot;VCO&quot;),  
		NOT(CONTAINS(UPPER({!myVariable_current.Name}) ,&quot;QATEST&quot;)),   
		OR(
			AND( 
				NOT(CONTAINS(UPPER({!myVariable_current.Rate_Plan_Description__c}) ,&quot;PRO_12.99_2X&quot;)),
				NOT(CONTAINS(UPPER({!myVariable_current.Rate_Plan_Description__c}) ,&quot;PRO_14.99_2X&quot;)),
				NOT(CONTAINS(UPPER({!myVariable_current.Rate_Plan_Description__c}) ,&quot;SMB1_29.99_5X&quot;)),				
				NOT(CONTAINS(UPPER({!myVariable_current.Rate_Plan_Description__c}) ,&quot;ITUNES&quot;)), 
				NOT(CONTAINS(UPPER({!myVariable_current.Rate_Plan_Description__c}) ,&quot;ANDROID&quot;))  
			), ISBLANK({!myVariable_current.Rate_Plan_Description__c}) )    , 
		NOT(CONTAINS(UPPER({!myVariable_current.Email_Addresses_1__c}) ,&quot;@J2.COM&quot;)), 
		NOT(ISPICKVAL({!myVariable_current.Status__c}, &quot;Inactive&quot;)), 
		NOT(ISPICKVAL({!myVariable_current.Status__c}, &quot;Deactivated&quot;)), 
		NOT(ISPICKVAL({!myVariable_current.Status__c}, &quot;Suspended&quot;)), 
		NOT(ISPICKVAL({!myVariable_current.Status__c}, &quot;Closed&quot;)) 
	)</expression>
    </formulas>
